#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5608ffd54030 .scope module, "pwm_control_tb" "pwm_control_tb" 2 1;
 .timescale 0 0;
v0x5608ffd9c7a0_0 .var "clk", 0 0;
v0x5608ffd9c870_0 .net "led", 0 0, v0x5608ffd9c680_0;  1 drivers
S_0x5608ffd541b0 .scope module, "dut" "pwm_control" 2 6, 3 1 0, S_0x5608ffd54030;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /OUTPUT 1 "led"
v0x5608ffd65fe0_0 .net "clk", 0 0, v0x5608ffd9c7a0_0;  1 drivers
v0x5608ffd9c4e0_0 .var "counter", 7 0;
v0x5608ffd9c5c0_0 .var "duty_cycle", 7 0;
v0x5608ffd9c680_0 .var "led", 0 0;
E_0x5608ffd8b2f0 .event posedge, v0x5608ffd65fe0_0;
    .scope S_0x5608ffd541b0;
T_0 ;
    %wait E_0x5608ffd8b2f0;
    %load/vec4 v0x5608ffd9c4e0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5608ffd9c4e0_0, 0;
    %load/vec4 v0x5608ffd9c5c0_0;
    %load/vec4 v0x5608ffd9c4e0_0;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5608ffd9c680_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5608ffd9c680_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5608ffd541b0;
T_1 ;
    %wait E_0x5608ffd8b2f0;
    %load/vec4 v0x5608ffd9c4e0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x5608ffd9c5c0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5608ffd9c5c0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5608ffd9c4e0_0;
    %pad/u 32;
    %cmpi/e 255, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x5608ffd9c5c0_0;
    %subi 1, 0, 8;
    %assign/vec4 v0x5608ffd9c5c0_0, 0;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5608ffd54030;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5608ffd9c7a0_0;
    %inv;
    %store/vec4 v0x5608ffd9c7a0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5608ffd54030;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5608ffd9c7a0_0, 0, 1;
    %delay 10, 0;
    %delay 100, 0;
    %vpi_call 2 20 "$display", "Increasing brightness..." {0 0 0};
    %pushi/vec4 255, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10, 0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %delay 100, 0;
    %vpi_call 2 26 "$display", "Decreasing brightness..." {0 0 0};
    %pushi/vec4 255, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10, 0;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tled.v";
    "led.v";
